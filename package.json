    "@angular/router": "3.0.0-alpha.8",
    "concurrently": "^2.1.0",
    "core-js": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^2.3.2",
    "ie-shim": "^0.1.0",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "~0.6.12"
  },
  "keywords": [
    "Angular2",
    "Components library"
  ],
